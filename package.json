{
  "name": "vite-webpack-bundle-analyzer",
  "version": "1.0.2",
  "type": "module",
  "license": "MIT",
  "description": "Vite plugin that represents bundle content as convenient interactive zoomable treemap from Webpack",
  "scripts": {
    "build": "tsup"
  },
  "files": ["dist", "README.md"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "peerDependencies": {
    "vite": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "4.5.0",
    "tsup": "7.2.0"
  },
  "dependencies": {
    "webpack-bundle-analyzer": "4.9.1",
    "@types/webpack-bundle-analyzer": "4.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "bundle",
    "analyzer",
    "modules",
    "size",
    "interactive",
    "chart",
    "treemap",
    "zoomable",
    "zoom"
  ]
}
